<div class="wrapper skin-blue">
  <aside class="right-side strech">
    {{#if isFrameEnabled}}
      <iframe {{bind-attr src="iframeSrc"}} class="iframe-content"></iframe>
    {{/if}}
    <section class="content-header">
      <h1>
        Horizon
        <small>OpenStack Dashboard
          <a href="#/app/horizon" class="hint--top" data-hint="Open in new window" target="_blank">
            <i class="fa fa-external-link"></i>
          </a>
        </small>
      </h1>
      <ol class="breadcrumb">
        <li>{{#link-to "application"}}<i class="fa fa-dashboard"></i> Home{{/link-to}}</li>
        <li class="active">Horizon</li>
      </ol>
    </section>
  </aside>
</div>
{{#unless isFrameEnabled}}
  {{partial "app/-disabled-app"}}
{{/unless}}
